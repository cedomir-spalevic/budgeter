{
   "name": "budgeter",
   "version": "1.3.2",
   "private": true,
   "scripts": {
      "clean": "npm-run-all clean:*",
      "clean:watchman": "watchman watch-del-all",
      "clean:dependencies": "rm -rf node_modules",
      "clean:reinstall": "npm install --ignore-engines",
      "clean:babel-cache": "sudo rm -rf node_modules/.cache/babel-loader/*",
      "build": "npm run build:format && npm run build:lint",
      "build:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
      "build:format": "prettier --write .",
      "android": "react-native run-android",
      "ios": "react-native run-ios",
      "start": "react-native start",
      "postversion": "react-native-version",
      "push-release": "git push --follow-tags",
      "generate-splash": "react-native generate-bootsplash app-icon.png --background-color=1882c4"
   },
   "dependencies": {
      "@react-hook/merged-ref": "^1.3.0",
      "@react-native-community/checkbox": "^0.5.6",
      "@react-native-community/masked-view": "^0.1.10",
      "@react-native-community/toolbar-android": "0.1.0-rc.2",
      "@react-native-picker/picker": "^1.9.11",
      "@react-navigation/bottom-tabs": "^5.5.0",
      "@react-navigation/native": "^5.4.3",
      "@react-navigation/stack": "^5.4.0",
      "@reduxjs/toolkit": "^1.5.0",
      "expo-local-authentication": "^9.3.0",
      "formik": "^2.2.6",
      "libphonenumber-js": "^1.9.16",
      "moment": "^2.29.1",
      "react": "16.11.0",
      "react-native": "^0.63.2",
      "react-native-bootsplash": "^3.1.5",
      "react-native-confirmation-code-field": "^6.5.1",
      "react-native-date-picker": "^3.2.9",
      "react-native-dotenv": "^2.5.3",
      "react-native-fs": "^2.16.6",
      "react-native-gesture-handler": "^1.7.0",
      "react-native-keyboard-accessory": "^0.1.14",
      "react-native-linear-gradient": "^2.5.6",
      "react-native-notifications": "^3.4.1",
      "react-native-paper": "^4.5.0",
      "react-native-popup-menu": "^0.15.10",
      "react-native-reanimated": "^1.13.0",
      "react-native-root-toast": "^3.2.1",
      "react-native-safe-area-context": "^3.1.4",
      "react-native-screens": "^2.10.1",
      "react-native-secure-storage": "^0.1.2",
      "react-native-simple-dialogs": "^1.3.0",
      "react-native-svg": "^12.1.0",
      "react-native-swipeable": "^0.6.0",
      "react-native-tab-view": "^2.14.2",
      "react-native-unimodules": "^0.11.0",
      "react-native-vector-icons": "^6.7.0",
      "react-redux": "^7.2.2",
      "redux-thunk": "^2.3.0",
      "yup": "^0.32.8"
   },
   "devDependencies": {
      "@babel/core": "^7.6.2",
      "@babel/runtime": "^7.12.5",
      "@bam.tech/react-native-make": "^3.0.0",
      "@react-native-community/eslint-config": "^1.0.0",
      "@types/jest": "^24.0.24",
      "@types/react-native": "^0.62.0",
      "@types/react-redux": "^7.1.16",
      "@types/react-test-renderer": "16.9.2",
      "@typescript-eslint/eslint-plugin": "^4.18.0",
      "@typescript-eslint/parser": "^4.18.0",
      "babel-jest": "^24.9.0",
      "babel-plugin-module-resolver": "^4.0.0",
      "babel-plugin-root-import": "^6.5.0",
      "chalk": "^4.1.0",
      "eslint": "^7.22.0",
      "eslint-plugin-react": "^7.22.0",
      "jest": "^24.9.0",
      "metro-react-native-babel-preset": "^0.58.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.1",
      "react-native-version": "^4.0.0",
      "react-test-renderer": "16.11.0",
      "typescript": "^3.8.3"
   },
   "jest": {
      "preset": "react-native",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   },
   "config": {
      "environment": "dev"
   }
}
